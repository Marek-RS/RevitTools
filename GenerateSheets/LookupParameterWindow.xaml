<Window x:Class="TTTRevitTools.GenerateSheets.LookupParameterWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TTTRevitTools.GenerateSheets"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        WindowStartupLocation="CenterOwner"
        Title="LookupParameterWindow" Height="200" Width="250">
    <Grid Margin="5">
        <GroupBox BorderBrush="Gray">
            <GroupBox.Header>
                Select Parameter
            </GroupBox.Header>
            <Grid>
                <ComboBox x:Name="ComBoxLookup"  Margin="10" VerticalAlignment="Top" />
                <Button Margin="10,35" VerticalAlignment="Bottom" Content="OK" Click="BtnClick_OK"></Button>
                <Button Margin="10" VerticalAlignment="Bottom" Content="Cancel" Click="BtnClick_Cancel"></Button>
            </Grid>
        </GroupBox>
    </Grid>
</Window>
